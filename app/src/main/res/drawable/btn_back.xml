<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape  android:shape="oval">
            <solid android:color="@color/yelow_fondo"/>
            <stroke android:color="@color/white"
                android:width="0.5dp"/>
            <item android:drawable="@drawable/ic_baseline_keyboard_arrow_down_24"
                android:gravity="center"
                android:color="@color/color_palceholder"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_keyboard_backspace_24"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:color="#ffffff"/>
</layer-list>